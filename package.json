{
  "name": "@lnfusion/gatsby-source-graphql",
  "description": "Gatsby plugin which adds a third-party GraphQL API to Gatsby GraphQL with whitelabels",
  "version": "0.0.3",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@graphql-tools/links": "^7.0.3",
    "@graphql-tools/utils": "^7.0.2",
    "@graphql-tools/wrap": "^7.0.1",
    "apollo-link": "1.2.14",
    "apollo-link-http": "^1.5.17",
    "dataloader": "^2.0.0",
    "invariant": "^2.2.4",
    "node-fetch": "^2.6.1",
    "uuid": "3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "babel-preset-gatsby-package": "^0.11.0-next.0",
    "cross-env": "^7.0.3"
  },
  "homepage": "https://github.com/lnfusion/gatsby-source-graphql#readme",
  "bugs": {
    "url": "https://github.com/lnfusion/gatsby-source-graphql/issues"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": "^2.0.15"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lnfusion/gatsby-source-graphql.git"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
